<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="Generic_Text" >
        <item name="android:textSize">@dimen/font_text</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>

    </style>

    <style name="Generic_Text_Title">
        <item name="android:textSize">@dimen/font_title</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">match_parent</item>

        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Generic_Text_Note">
        <item name="android:textSize">@dimen/font_notes</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">wrap_content</item>
    </style>





</resources>